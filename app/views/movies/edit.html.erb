<h1>Editing movie</h1>

<div class="card col-lg-6 col-md-6 col-sm-12 col-xs-12">
  <div class="card-body">
    <%= render "form", movie: @movie %>
    <br>
    <div class="btn-group btn-group-toggle" data-toggle="buttons">
      <%= link_to "Show this movie", @movie, class:"btn btn-primary btn-sm m-1" %>
      <%= link_to "Back to movies", movies_path, class:"btn btn-primary btn-sm m-1" %>
    </div>
  </div>
</div>
